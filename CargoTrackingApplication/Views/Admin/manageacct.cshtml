
@{
    ViewBag.Title = "manageacct";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


@{
    ViewBag.Title = "clientreg";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



@Html.Partial("_AdminHeader")

<!--main content begins -->
<div class="ed_transprentbg ed_toppadder90 ed_bottompadder90" style="back">
    <div class="container content">
        <div class="row">
            <!-- left side bar for quick links -->
            <div class="col-lg-3 col-md-3 col-sm-3 adminsidebar">
                <h3 style="font-weight: bolder;COLOR:#FFF;">QUICK LINKS</h3>

                @Html.Partial("_HeaderLink")

            </div>
            <!-- left side bar for quick links ends -->
            <!-- right side bar for tracking details and feedback -->
            <div class="col-lg-9 col-md-9 col-sm-9 admincreateidbar">
                <hgroup>
                    <h4 style="font-weight:bolder;"><i class="fa fa-user"></i> Update Password</h4>
                    <br>
                </hgroup>
                <div class="error_detect"> </div>
                <form panel panel-group createtrackid action="" method="post">
                    <div class="form-group">
                        <input class="form-control" type="email" placeholder="Email" name="email" required />
                    </div>
                    <div class="form-group">
                        <input class="form-control" type="password" placeholder="New password" name="newpass" required />
                    </div>
                    <div class="form-group">
                        <input class="form-control" type="password" placeholder="Confirm password" name="confampass" required />
                    </div>

                    <button class="btn btn-primary" name="update" type="submit"><i class="fa fa-paper-plane"></i>Update</button>
                </form>
            </div>
        </div>



    </div>

    <!-- right sidebar tracking details and feedback ends-->
</div>

